{
  "name": "gdrivejs",
  "version": "1.0.4",
  "description": "A lightweight Bun + TypeScript package for Google Drive OAuth2 authentication and file operations.",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "dev": "bun run index.ts",
    "build": "bun build ./index.ts --outdir dist --target node",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "google-drive",
    "bun",
    "typescript",
    "googleapis",
    "oauth2"
  ],
  "author": "Vikash Khati",
  "license": "MIT",
  "dependencies": {
    "googleapis": "^164.1.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/node": "latest"
  },
  "files": [
    "dist"
  ]
}
